{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/components/icons/*"],
      "@data/*": ["src/data/*"],
      "@config": ["src/config.ts"],
      "@utils": ["src/utils.ts"]
    }
  }
}
